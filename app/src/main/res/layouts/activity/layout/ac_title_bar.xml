<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.wuhai.myframe2.widget.titlebar2.TitleViewWithBack
        android:id="@+id/titleview_with_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.wuhai.myframe2.ui.autowrap.widget.TitleBar
        android:id="@+id/titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.wuhai.myframe2.widget.titlebar2.TitleViewWithCloseBtn
        android:id="@+id/titleview_with_close"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>
